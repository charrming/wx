<!--index.wxml-->
<view class="container">
  <view class="top-cover">
    <view class="top-top">
      <view class="top-left">
        <view class="img-cover" bindtap="uploadFile">
        <image mode="widthFix" class="top-left-img" src='data:image/jpg;base64,'></image>      
        </view>
      </view>
      <view class="top-right">
        <view class="top-right-top">
          404 NOT FOUND
        </view>
        <view class="top-right-bottom">
          <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo" class="loginbtn"> 获取头像昵称 </button>
          <block wx:else>
            <view class="userinfo" bindtap="showMyList">
              <image id="avatar" class="userinfo-avatar {{musicswitch ? 'gorotate':''}}" src="{{userInfo.avatarUrl}}" mode="cover"></image>
              <text class="userinfo-nickname">{{userInfo.nickName}}</text>              
            </view>
          </block>
        </view>
        <view class="mode">          
          <text class="mode-icon iconfont icon-shuzishunxu {{play_mode==1?'mode-now':''}}" bindtap="changeMode" data-mode="1"></text>
          <text class="mode-icon iconfont icon-danquxunhuan {{play_mode==2?'mode-now':''}}" bindtap="changeMode" data-mode="2"></text>
          <text class="mode-icon iconfont icon-suiji {{play_mode==3?'mode-now':''}}" bindtap="changeMode" data-mode="3"></text>
          <text class="mode-icon iconfont icon-renyuan {{play_mode==4?'mode-now-green':''}}"></text>
          <text wx:if="{{music_now!=0}}" bindtap="changeNextSong" class="mode-icon iconfont icon-xiayishou"></text>
        </view>       
      </view>      
    </view>
    <view class="top-bottom">
      <view class="comment-list-title">留言板<text class="comment-item-date">({{comment_list.length||0}}条)</text><text class="iconfont icon-shangchuan-copy" bindtap="showUpload"></text><text class="iconfont icon-xiepinglun" bindtap="showComment"></text></view>
      <view class="comment-list-cover">
        <block wx:if="{{comment_list.length>0}}">
          <view class="comment-item" wx:for="{{comment_list}}" wx:key="item" wx:index="index">
            <view><image class="comment-item-avatar" src="{{item.avatar}}"></image><text class="comment-item-name">{{item.name}}</text><text class="comment-item-date">{{item.date}}</text></view>
            <view class="comment-item-content">{{item.comment}}</view>
          </view>
        </block>
        <block wx:else><view class="comment-item">暂无留言</view></block>
      </view>
      <view class="comment-view-cover {{comment_view?'comment-view-show':''}}">
        <view class="comment-view {{maskShow?'mask':''}}">
          <view class="comment-view-title">发表评论</view>
          <textarea placeholder="听了这么多，可能你有话想说" value="{{comment}}" bindinput="setComent"></textarea> 
          <view class="comment-view-btn-set">
            <button catchtap="sendComment">发送</button>
            <button catchtap="hideComment">再想想</button>
          </view>
        </view>
      </view>
      <view class="comment-view-cover {{upload_view?'comment-view-show':''}}">
        <view class="comment-view {{maskShow?'mask':''}}">
          <view class="comment-view-title">上传music</view>
          <view>
            <input type="text" class="upload-view-input" placeholder="暗号" value="{{upload_key}}" data-name="upload_key" bindinput="setUpload" placeholder-class="place-holder"/>
          </view>
          <block wx:if="{{upload_key==correct_key}}">
              <view>
                <input type="text" class="upload-view-input" placeholder="请输入歌名" value="{{upload_title}}" data-name="upload_title" bindinput="setUpload" placeholder-class="place-holder"/>
              </view>
              <view>
                <input type="text" class="upload-view-input" placeholder="请输入歌手名" value="{{upload_singer}}" data-name="upload_singer" bindinput="setUpload" placeholder-class="place-holder"/>
              </view>
              <button catchtap="selectFile" style="margin-bottom: 16rpx;">{{upload_button_name}}</button>

          </block>
          <view class="comment-view-btn-set">
            <button catchtap="sendUpload" style="background-color:#b31c1c;" wx:if="{{upload_key==correct_key}}">上传</button>
            <button catchtap="hideUpload" style="background-color: white;color: #131313;">取消</button>
          </view>
        </view>
      </view>
      <!-- <view><bar></bar></view> -->
    </view>
  </view>
  <view class="bottom-cover">
    <view> 
      <block wx:for="{{music_list}}" wx:key="item" wx:index="index">
        <view class="music-list {{index%2==0?'gray-bg':''}}" bindtap="setMyList" data-item="{{item}}">
          <view class="music-list-num">{{index+1}}</view>
          <view class="music-list-title">{{item.title}}</view>
          <view class="music-list-singer">{{item.singer}}</view>
          <text class="iconfont icon-zanting play-btn" wx:if="{{music_now._id==item._id&&!musicIsPause&&play_mode!=4}}" catchtap="pauseMusic"></text>
          <text class="iconfont icon-arrow- play-btn" wx:else catchtap="selectMusic" data-item="{{item}}" data-index="{{index}}"></text>
        </view>
      </block> 
    </view>    
  </view>
</view>
<view class="my-list-cover {{my_list_show?'my-list-show':''}}" mut-bind:tap="hideMyList">
  <view class="my-list" mut-bind:tap="nothing">
    <view class="my-list-play">
      <view bindtap="playMyList"><text class="iconfont icon-bofang_huaban play-btn"></text> 播放全部(共{{my_list.length}}首)</view>
    </view>
    <view class="music-list-cover"> 
      <block wx:for="{{my_list}}" wx:key="item" wx:index="index">
        <view class="music-list {{index%2==0?'my-list-gray-bg':''}}">
          <view class="music-list-num" data-item="{{item}}"><text class="iconfont icon-yinpin" wx:if="{{music_now._id==item._id&&play_mode==4}}"></text><text wx:else>{{index+1}}</text></view>
          <view class="music-list-title">{{item.title}}</view>
          <view class="music-list-singer">{{item.singer}}</view>
          <text bindtap="removeMyList" data-index="{{index}}" class="iconfont icon-shanchu play-btn"></text>
        </view>
      </block> 
    </view>  
  </view>
</view>

